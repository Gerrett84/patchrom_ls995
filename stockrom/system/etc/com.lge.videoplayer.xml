<?xml version='1.0' encoding='utf-8' standalone='yes' ?>

<!--
    This file is used to declare music configuration depend on specific model
-->
<video_configration>
<!--Selection Log Level 
	Parms: debug, info, error or none
	Default value is debug-->
<log_level data="debug" />

<!--Selection Audio Effect Type 
	Parms: dolby, virtualSurround or cinemaSound or no_audio_effect scr
	Default value is lge_audio_effect
	value define : no_audio_effect/lge_audio_effect/virturesurround_effect/cinemasound_effect -->
<audio_effect data = "no_audio_effect">
</audio_effect>	
<!--Enable or Disable zoom gesture.
	Default value is false-->
<no_support_zoom_gesture data="false" />
<!--Enable or Disable divx.
	Default value is support_divx
	profile_value define : no_support_divx/support_divx/mobile/mobiletheater-->
<divx data = "support_divx" extra_divx_disabled_operator = "DCM,KDDI">
</divx>
<!--Support for Gesture Guide.
	Default value is false-->
<no_support_gesture_guide data="false" />
<!--Support for Gesture.
	Default value is false-->
<no_support_gesture data="false" />
<!--Enable or Disable MultiThread thumbnail.
	Default value is false-->
<multithread_thumb data="true" />
<!--Support for ListMenu Key.
	Default value is false-->
<no_support_listmenu_key data="false" />
<!--Support for HDMI.
	Default value is false-->
<no_support_hdmi data="false" />
<!--Support Full Mirroring HDMI.
	Default value is false-->
<not_full_mirroring_hdmi data="false" />
<!--Support HDMI Audio.
	Default value is false-->
<support_hdmi_audio data="false" />
<!--Support SENSOR_TYPE_ACCELEROMETER.
	Default value is false-->
<accelerometer_for_sensor_type data="false" />
<!--Support RECENT KEY.
	Default value is 0
	value define : 0/13/14-->
<recentkey_value data="0" />
<!--Support 3D.
	Default value is false-->
<support_3d data="false" />
<!--Support LANDSCAPE_MODE_ONLY_SUPPORTED_IN_HDMI.
	Default value is false-->
<hdmi_with_only_landscape_mode data="false" />
<!--Support DSDR
	Default value is false-->
<support_dsdr data="true" />
<!--Support NUMERIC_SORT_SUPPORT
	Default value is false-->
<support_numeric_sort data="true" />
<!--Support OVERLAY_SUPPORT
	Default value is false-->
<no_support_overay_model data="false" />
<!--Support AUDIOZOOM_SUPPORT
	Default value is false-->
<support_audio_zoom data="true" />
<!--Support IS_RMS_SUPPORTED
	Default value is false-->
<support_rms data="true" />
<!--Support IS_WFD_DLNA_CONVERT
	Default value is false-->
<support_wfd_dlna_convert data="true" />
<!--Support IS_EXCEPTION_MODEL_OF_ENTRY_CALLING
	Default value is false-->
<exception_model_of_entry_calling data="false" />
<!--Support IS_ENHVIDEO_SPEED_CTRL
	Default value is false-->
<enhvideo_speed_ctrl data="true" />
<!--Support IS_ENHVIDEO_THUMBNAIL_PLIST
	Default value is false-->
<enhvideo_thumbnail_list data="true" />
<!--Support IS_ENHVIDEO_FINGERTIP_PREVIEW_SEEKBAR
	Default value is false-->
<enhvideo_fingertip_preview_seekbar data="true" />
<!--Support IS_SLIMPORT
	Default value is false-->
<support_sllimport data="true" />
<!--Support IS_HARDKEYLESS_SUPPORTED
	Default value is false-->
<support_hardkeyless data="true" />
<!--Support IS_NEW_AUDIO_ZOOM_FRAMEWORK
	Default value is false-->
<new_audio_zoom_framework data="true" />
<!--Support IS_VIDEO_AUTO_OFF_SUPPORT 
	Default value is false-->
<support_video_auto_off data="true" />
<!--Support IS_SMARTTRACKING_SUPPORT 
	Default value is false-->
<support_smart_tracking data="true" />
<!--Support IS_NEW_DSDP_FRAMEWORK 
	Default value is false-->
<new_dsdp_framework data="true" />
<!--Support IS_FILELOCK_FEAT_SUPPORTED 
	Default value is false-->
<support_filelock data="false" />
<!--Support IS_MULTIPLE_AUDIOTRACKS 
	Default value is false-->
<support_multiple_audiotracks data="true" />
<!--Support IS_SPLIT_VIEW_SUPPORTED 
	Default value is false-->
<support_split_view data="true" />
<!--Support IS_SUPPORT_ONEHANDEDOPERATION 
	Default value is false-->
<support_one_handed_operation data="false" />
<!--Support IS_SCREEN_ROTATION_SUPPORTED 
	Default value is false-->
<support_screen_rotation data="true" />
<!--Support IS_SUPPORT_PLAY_ON_LOCKSCREEN 
	Default value is false-->
<support_list_pinch data="false" />
<!--Support IS_SUPPORT_TO_DO_AFTER_PLAY 
	Default value is false-->
<support_to_do_after_play data="true" />
<!--Support IS_SUPPORT_PLAY_ON_LOCKSCREEN 
	Default value is false-->
<support_play_on_lockscreen data="true" />
<!--Support IS_SUPPORT_SMARTVIDEO 
	Default value is false-->
<support_smartvideo data="true" />
<!-- Support support_native_dsdp Z default is true-->
<support_native_dsdp data="true" />
</video_configration>
