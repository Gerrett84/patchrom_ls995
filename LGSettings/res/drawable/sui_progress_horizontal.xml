<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#ff797979" android:endColor="#ff646f7f" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff606c7c" />
        </shape>
    </item>
    <item android:id="@*android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#ff797979" android:endColor="#ff646f7f" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff606c7c" />
            </shape>
        </clip>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#ff02edea" android:endColor="#ff08baba" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff05d5d2" />
            </shape>
        </clip>
    </item>
</layer-list>